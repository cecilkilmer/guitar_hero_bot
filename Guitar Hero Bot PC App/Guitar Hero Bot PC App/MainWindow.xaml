<Window x:Class="Guitar_Hero_Bot_PC_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:c="clr-namespace:Guitar_Hero_Bot_PC_App"
        Title="MainWindow" Height="190.919" Width="548.534">
    <Grid x:Name="MainGrid">
        <Button Content="Load File" HorizontalAlignment="Left" Margin="443,10,0,0" VerticalAlignment="Top" Width="89" Click="Click_Load_File"/>
        <Button Content="Parse File" HorizontalAlignment="Left" Margin="443,45,0,0" VerticalAlignment="Top" Width="89"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="{Binding FileText}" VerticalAlignment="Top" Width="420"/>
        <TextBox HorizontalAlignment="Left" Height="90" Margin="10,60,0,0" TextWrapping="Wrap" Text="{Binding StatusText}" VerticalAlignment="Top" Width="420"/>
        <Button Content="Start Playback" HorizontalAlignment="Left" Margin="443,131,0,0" VerticalAlignment="Top" Width="89"/>

    </Grid>
</Window>
